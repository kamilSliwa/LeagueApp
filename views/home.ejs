<div class="row">
  <div class="col-xs-6 col-lg-9 homeTable">
    <table class="table">
      <thead>
        <tr class="table-primary">
          <th scope="col">Pozycja</th>
          <th scope="col">Nazwa drużyny</th>
          <th scope="col">Mecze</th>
          <th scope="col">Punkty</th>
        </tr>
      </thead>
      <% let meter=0;%> <%byPoints.forEach(byPoints=>{%> <% meter=meter+1;%>

      <tbody>
        <tr>
          <th scope="row"><%=meter%></th>
          <td><%=byPoints.name%></td>
          <td><%=byPoints.played()%></td>
          <td><%=byPoints.points%></td>
        </tr>
      </tbody>

      <%});%>
    </table>
  </div>
  <div class="col-xs-6 col-lg-3 shootersLead">
    <table class="table">
      <thead>
        <tr class="table-primary">
          <th scope="col"></th>
          <th scope="col">Zawodnik</th>

          <th scope="col">Bramki</th>
        </tr>
      </thead>
      <% let meters=0;%> <%sortByGoals.forEach(byGoals=>{%> <%
      meters=meters+1;%>

      <tbody>
        <tr>
          <th scope="row"><%=meters%></th>
          <td><%=byGoals.fname%> <%=byGoals.lname%></td>

          <td><%=byGoals.goals%></td>
        </tr>
      </tbody>

      <%});%>
    </table>
  </div>
</div>
<div class="row">
  <% if(logged==true){%>
  <div>
    <a href="/addMatch" class="btn btn-success">Dodaj mecz</a>
    <a href="/teams" class="btn btn-success">Dodaj drużyne</a>
  </div>

  <%}%>
</div>
